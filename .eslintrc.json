{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
      },
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "overrides": [],
    "plugins": ["react", "prettier"],
    "rules": {
        "react/prop-types": 0,
        "prettier/prettier": "error",
        "react/react-in-jsx-scope": "off"
    }
}
